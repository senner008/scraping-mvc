<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - scraping_mvc</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <script src="https://kit.fontawesome.com/d7b24ba5c7.js" crossorigin="anonymous"></script>
</head>
<body>
    <div class="preload-font-awesome-hidden" style="width:0; height:0; overflow:hidden;">
        <i name="database" class="fas fa-database"></i>
        <i name="node-js" class="fab fa-node-js"></i>
        <i name="sort-down" class="fas fa-sort-amount-down-alt"></i>
        <i name="sort-up" class="fas fa-sort-amount-up-alt"></i>
    </div>
    <header>
       <div id="header-wrapper">
            <div id="titleText">
                <div class="input-container header mb-3 btn-group btn-group-toggle" data-toggle="buttons">   
                    <button class="icon-toggle jsdb input-group-text" style="background: white"><i class="fas fa-database"></i></button>
                    <h1 class="label-align-class font-increase btn btn-secondary active">
                        <div>Søg i Menu</div>
                        <div id="process-information"><div>Loading...  </div><div class="spinner-icon spinner-border spinner-border-sm"></div></div>
                    </h1>
                </div>
            </div> 
       </div>
    </header>
    <div id="main" >
        <div  class="container">
           
            @RenderBody()
    
        </div>
      
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2019 - scraping_mvc - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/bundle.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
