@{
    ViewData["Title"] = "Home Page";
}

        <div id="titleText">
            <div class="input-container header mb-3 btn-group btn-group-toggle" data-toggle="buttons">   
                    <button class="icon-toggle jsdb input-group-text" style="background: white"><i class="fas fa-database"></i></button>
                    <h1 class="label-align-class font-increase btn btn-secondary active">
                       <div>Søg i Menu</div>
                       <div id="process-information"><div>Loading...  </div><div class="spinner-icon spinner-border spinner-border-sm"></div></div>
                    </h1>
              
                </div>
       
            <!-- <button id="spinnerButton" class="btn btn-primary">
                <span class="spinner-border spinner-border-sm"></span>
            </button> -->
        </div> 
       
         <div class="form-group">

             <div class="form-group row">
                  <div class="input-container col-md-4 mb-3">
                    <input class="form-control" id="categorySearch" placeholder="Fra kategori..." type="text" name="name" value="" />
                </div>
                 <div class="input-container col-md-4 mb-3">
                    <input class="form-control" id="titleSearch" placeholder="Titel eller nummer..." type="text" name="name" value="" />
                </div>
               <div class="input-container col-md-4 mb-3">
                    <input class="form-control" id="priceSearch" placeholder="Indtast max pris..." type="number" name="name" value="" />
                </div>
                  <div class="input-container col-md-4 mb-3">
                    <input class="form-control" id="descriptionSearch" placeholder="Indeholder..." type="text" name="name" value="" />
                </div>
                 <div class="input-container col-md-4 mb-3">
                    <select id="lunchSelect" class="custom-select my-1 mr-sm-2" id="inlineFormCustomSelectPref">
                        <option value="1">Standard</option>
                        <option value="2">Frokost tilbud</option>
                    </select>
                </div>
                <div class="input-container col-md-4 mb-3 btn-group btn-group-toggle" data-toggle="buttons">   
                    <button class="icon-toggle sort input-group-text" style="background: white"><i class="sort-direction-down fas fa-sort-amount-down-alt"></i></button>
                    <label class="label-align-class sort btn btn-secondary active">
                        <input type="radio" name="category" id="option1" autocomplete="off" checked>Kategori
                    </label>
                    <label class="label-align-class sort btn btn-secondary">
                        <input type="radio" name="title" id="option2" autocomplete="off">Titel
                    </label>
                    <label class="label-align-class sort btn btn-secondary">
                        <input type="radio" name="price" id="option3" autocomplete="off">Pris
                    </label>
                </div>
            </div>
        <ul class="list-group" id="listUl"></ul>
    </div>
